{
    "diffEditor.codeLens": true,
    "diffEditor.renderSideBySide": false,
    "diffEditor.wordWrap": "inherit",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingDelete": "always",
    "editor.autoClosingQuotes": "always",
    "editor.autoIndent": "full",
    "editor.bracketPairColorization.enabled": true,
    "editor.codeLens": true,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "line",
    "editor.detectIndentation": true,
    "editor.dragAndDrop": true,
    "editor.emptySelectionClipboard": true,
    "editor.folding": true,
    "editor.fontFamily": "'FuraMono NF', 'Fira Mono', 'Fira Code','Hack','Droid Sans Fallback',monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 15,
    "editor.fontWeight": "normal",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "editor.inlineSuggest.enabled": true,
    "editor.insertSpaces": true,
    "editor.largeFileOptimizations": true,
    "editor.letterSpacing": 0,
    "editor.lineNumbers": "on",
    "editor.linkedEditing": true,
    "editor.minimap.enabled": true,
    "editor.minimap.maxColumn": 120,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.minimap.side": "right",
    "editor.minimap.size": "proportional",
    "editor.renderFinalNewline": true,
    "editor.renderIndentGuides": true,
    "editor.rulers": [
        80,
        120
    ],
    "editor.showDeprecated": true,
    "editor.suggest.preview": true,
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "security.workspace.trust.banner": "untilDismissed",
    "security.workspace.trust.emptyWindow": true,
    "security.workspace.trust.startupPrompt": "always",
    "workbench.commandPalette.history": 25,
    "workbench.colorTheme": "One Dark Pro Darker",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.labelFormat": "short",
    "workbench.editor.languageDetection": false,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 10,
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.settings.editor": "json",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "none",
    "files.defaultLanguage": "markdown",
    "files.encoding": "utf8",
    "files.eol": "\n",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true
    },
    "files.insertFinalNewline": true,
    "files.maxMemoryForLargeFilesMB": 4096,
    "files.trimTrailingWhitespace": true,
    "explorer.sortOrder": "mixed",
    "search.followSymlinks": false,
    "search.maxResults": 10000,
    "search.showLineNumbers": true,
    "search.useIgnoreFiles": true,
    "update.mode": "manual",
    "update.showReleaseNotes": false,
    "html.autoClosingTags": true,
    "html.format.wrapLineLength": 120,
    "html.suggest.html5": true,
    "markdown.preview.fontFamily": "'Verdana',-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
    "extensions.autoCheckUpdates": true,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "terminal.external.windowsExec": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontFamily": "'FuraMono NF', 'Fira Mono', 'Fira Code','Hack','Droid Sans Fallback',monospace",
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        }
    },
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },
    "breadcrumbs.enabled": true,
    "telemetry.telemetryLevel": "off",
    "remote.autoForwardPorts": true,
    "remote.downloadExtensionsLocally": true,
    "git.allowForcePush": false,
    "git.enableCommitSigning": true,
    "git.inputValidationLength": 80,
    "git.requireGitUserConfig": true,
    "xmlTools.removeCommentsOnMinify": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
    },
    "gitlens.blame.avatars": false,
    "gitlens.blame.compact": true,
    "gitlens.blame.heatmap.enabled": true,
    "gitlens.blame.highlight.locations": [
        "overview",
        "gutter",
        "line"
    ],
    "gitlens.codeLens.authors.enabled": true,
    "gitlens.codeLens.enabled": true,
    "gitlens.codeLens.recentChange.command": "gitlens.showQuickCommitDetails",
    "gitlens.codeLens.scopes": [
        "document"
    ],
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.hovers.enabled": false,
    "gitlens.liveshare.allowGuestAccess": false,
    "terraform.languageServer": {
        "external": true
    },
    "csharp.format.enable": false,
    "csharp.referencesCodeLens.enabled": false,
    "omnisharp.enableImportCompletion": true,
    "omnisharp.enableRoslynAnalyzers": true,
    "omnisharp.loggingLevel": "warning",
    "omnisharp.organizeImportsOnFormat": true,
    "python.languageServer": "Pylance",
    "python.linting.enabled": true,
    "python.linting.mypyEnabled": true,
    "python.linting.pylintEnabled": true,
    "remote.containers.defaultExtensions": [
        "aaron-bond.better-comments",
        "bungcip.better-toml",
        "christian-kohler.path-intellisense",
        "codezombiech.gitignore",
        "eamodio.gitlens",
        "ecmel.vscode-html-css",
        "naumovs.color-highlight",
        "Tyriar.sort-lines",
        "wayou.vscode-todo-highlight"
    ],
    "remote.SSH.defaultExtensions": [
        "aaron-bond.better-comments",
        "bungcip.better-toml",
        "christian-kohler.path-intellisense",
        "codezombiech.gitignore",
        "eamodio.gitlens",
        "ecmel.vscode-html-css",
        "naumovs.color-highlight",
        "Tyriar.sort-lines",
        "wayou.vscode-todo-highlight"
    ],
    "remote.SSH.enableX11Forwarding": false,
    "remote.SSH.remotePlatform": {
        "dev": "linux",
    },
    "remote.SSH.showLoginTerminal": true,
    "powershell.powerShellDefaultVersion": "Windows PowerShell (x86)",
    "autoDocstring.docstringFormat": "google",
    "material-icon-theme.activeIconPack": "vue",
    "redhat.telemetry.enabled": false,
    "rust-client.engine": "rls",
    "rust.all_targets": false,
    "rust.racer_completion": false,
    "todohighlight.defaultStyle": {
        "color": "#ff6600",
        "backgroundColor": "#282c34"
    },
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss",
        "**/*.tex",
        "**/*.cs",
        "**/*.rs"
    ],
    "todohighlight.keywords": [
        "TODO",
        "REVIEW",
        "ISSUE",
        "BUG",
        "CVE",
        "WORKAROUND",
        {
            "text": "FIXME",
            "color:": "#ff0000",
            "backgroundColor:": "#282c34"
        },
        {
            "text": "HACK",
            "color:": "#ff0000",
            "backgroundColor": "#282c34"
        }
    ],
    "todohighlight.isCaseSensitive": true,
    "todohighlight.maxFilesForSearch": 4096,
    "ansible.ansibleLint.enabled": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/target/**": true
    },
    "remote.containers.dockerPath": "podman",
    "diffEditor.ignoreTrimWhitespace": false
}
